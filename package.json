{
  "name": "sudoku-project",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "dev:debug": "next dev --turbo --inspect",
    "build": "next build --turbo && tsc && pnpm copy-cache-handler",
    "copy-cache-handler": "test -f cache-handler.mjs && cp cache-handler.mjs .next/cache-handler.mjs || echo 'No cache handler to copy'",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "quality": "pnpm lint && pnpm format:check && pnpm type-check",
    "quality:fix": "pnpm lint:fix && pnpm format",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "test:responsive": "./scripts/test-responsive.sh",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "playwright:install": "./scripts/install-playwright.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "browserslist": "^4.27.0",
    "core-js": "^3.46.0",
    "fast-sudoku-solver": "^1.1.22",
    "lodash": "^4.17.21",
    "next": "^16.0.0",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "regenerator-runtime": "^0.14.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/config-array": "^0.21.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@eslint/object-schema": "^2.1.7",
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/core-js": "^2.5.8",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/regenerator-runtime": "^0.13.8",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^10.0.0",
    "esbuild": "^0.25.11",
    "eslint": "^9.38.0",
    "eslint-config-next": "^15.5.6",
    "happy-dom": "^20.0.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vitest": "^3.2.4"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "not ie 11"
  ],
  "pnpm": {
    "overrides": {
      "vite": "^7.1.7",
      "rimraf": "^4",
      "eslint-config-next": "^15.5.4",
      "minimatch": "^10.0.1",
      "globals": "^15.11.0",
      "js-tokens": "^9.0.0",
      "happy-dom": "^20.0.5",
      "p-limit": "^3.1.0",
      "which": "^5.0.0",
      "nanoid": "^3.3.8",
      "postcss": "^8.4.49",
      "cross-spawn": "^7.0.5",
      "@eslint/plugin-kit": "^0.3.4",
      "esbuild": "^0.25.9",
      "@babel/runtime": "^7.26.0",
      "@typescript-eslint/eslint-plugin": "^8.45.0",
      "@typescript-eslint/parser": "^8.45.0",
      "@babel/helpers": "^7.26.10",
      "eslint": "^9.36.0",
      "@eslint/js": "^9.36.0"
    }
  },
  "packageManager": "pnpm@10.19.0"
}
