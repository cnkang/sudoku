{
  "name": "sudoku-project",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "dev:debug": "next dev --turbo --inspect",
    "build": "next build --turbo && tsc && pnpm copy-cache-handler && pnpm strip:sourcemaps",
    "copy-cache-handler": "test -f cache-handler.ts && cp cache-handler.ts .next/cache-handler.ts || echo 'No cache handler to copy'",
    "strip:sourcemaps": "node --experimental-strip-types scripts/strip-production-sourcemaps.ts",
    "start": "next start",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "oxlint": "oxlint",
    "type-check": "tsc --noEmit",
    "quality": "pnpm lint && pnpm format:check && pnpm type-check && pnpm oxlint",
    "quality:fix": "pnpm lint:fix && pnpm format && pnpm type-check",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:pbt": "PBT_PROPERTY_TIME_LIMIT_MS=${PBT_PROPERTY_TIME_LIMIT_MS:-2500} vitest run --reporter=verbose src/**/*.pbt.test.ts",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "test:responsive": "./scripts/test-responsive.sh",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "playwright:install": "./scripts/install-playwright.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "fast-sudoku-solver": "^3.0.0",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@commitlint/types": "^20.4.0",
    "@playwright/test": "^1.58.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "fast-check": "^4.5.3",
    "happy-dom": "^20.6.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxlint": "^1.48.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "not ie 11"
  ],
  "pnpm": {
    "overrides": {
      "postcss": "^8.5.6",
      "source-map": "^0.7.4"
    },
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "packageManager": "pnpm@10.29.2+sha512.bef43fa759d91fd2da4b319a5a0d13ef7a45bb985a3d7342058470f9d2051a3ba8674e629672654686ef9443ad13a82da2beb9eeb3e0221c87b8154fff9d74b8"
}
