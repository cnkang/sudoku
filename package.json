{
  "name": "sudoku-project",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "dev:debug": "next dev --turbo --inspect",
    "build": "next build --turbo && tsc && pnpm copy-cache-handler",
    "copy-cache-handler": "test -f cache-handler.ts && cp cache-handler.ts .next/cache-handler.ts || echo 'No cache handler to copy'",
    "start": "next start",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "oxlint": "oxlint",
    "type-check": "tsc --noEmit",
    "quality": "pnpm lint && pnpm format:check && pnpm type-check && pnpm oxlint",
    "quality:fix": "pnpm lint:fix && pnpm format && pnpm type-check",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:pbt": "PBT_PROPERTY_TIME_LIMIT_MS=${PBT_PROPERTY_TIME_LIMIT_MS:-2500} vitest run --reporter=verbose src/**/*.pbt.test.ts",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "test:responsive": "./scripts/test-responsive.sh",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "playwright:install": "./scripts/install-playwright.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "browserslist": "^4.28.1",
    "core-js": "^3.47.0",
    "fast-sudoku-solver": "^3.0.0",
    "lodash": "^4.17.21",
    "next": "^16.1.1",
    "next-pwa": "^5.6.0",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "regenerator-runtime": "^0.14.1",
    "winston": "^3.19.0",
    "workbox-webpack-plugin": "^7.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@commitlint/types": "^20.2.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/core-js": "^2.5.8",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/regenerator-runtime": "^0.13.8",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "esbuild": "^0.27.2",
    "fast-check": "^4.5.3",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxlint": "^1.38.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "not ie 11"
  ],
  "pnpm": {
    "overrides": {
      "vite": "^7.2.2",
      "rimraf": "^4",
      "eslint-config-next": "^15.5.9",
      "minimatch": "^10.0.1",
      "globals": "^15.11.0",
      "js-tokens": "^9.0.0",
      "happy-dom": "^20.0.10",
      "p-limit": "^3.1.0",
      "which": "^5.0.0",
      "nanoid": "^3.3.8",
      "postcss": "^8.5.6",
      "cross-spawn": "^7.0.5",
      "@eslint/plugin-kit": "^0.3.4",
      "esbuild": "^0.25.12",
      "@babel/runtime": "^7.26.0",
      "@typescript-eslint/eslint-plugin": "^8.47.0",
      "@typescript-eslint/parser": "^8.47.0",
      "@babel/helpers": "^7.26.10",
      "eslint": "^9.39.1",
      "@eslint/js": "^9.39.1",
      "glob": "^11.1.0"
    },
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}
