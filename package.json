{
  "name": "sudoku-project",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build && tsc",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "quality": "yarn lint && yarn format:check && yarn type-check",
    "quality:fix": "yarn lint:fix && yarn format",
    "test:index": "vitest run tests/index.spec.ts --environment=node",
    "test:ui": "vitest run src/components/__tests__/*.test.tsx --environment=happy-dom",
    "test:hooks": "vitest run src/hooks/__tests__/*.test.ts --environment=happy-dom",
    "test:api": "vitest run src/app/api/**/__tests__/*.test.ts --environment=node",
    "test:app": "vitest run src/app/__tests__/*.test.tsx --environment=happy-dom",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "test:responsive": "./scripts/test-responsive.sh",
    "test:mobile": "vitest run src/components/__tests__/*.responsive.test.tsx src/app/__tests__/*.responsive.test.tsx --environment=happy-dom",
    "test": "vitest run",
    "prepare": "husky"
  },
  "dependencies": {
    "browserslist": "^4.26.0",
    "core-js": "^3.45.0",
    "fast-sudoku-solver": "^1.1.21",
    "lodash": "^4.17.21",
    "next": "^15.5.0",
    "postcss": "^8.5.6",
    "punycode": "^2.3.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "regenerator-runtime": "^0.14.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/config-array": "^0.21.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@eslint/object-schema": "^2.1.6",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/core-js": "^2.5.8",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.0",
    "@types/punycode": "^2.1.4",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.7",
    "@types/regenerator-runtime": "^0.13.8",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^10.0.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "not ie 11"
  ],
  "resolutions": {
    "vite": "^6.2.6",
    "rimraf": "^4",
    "eslint-config-next": "^15.1.3",
    "minimatch": "^10.0.1",
    "globals": "^15.11.0",
    "js-tokens": "^9.0.0",
    "happy-dom": "^17.0.2",
    "p-limit": "^3.1.0",
    "which": "^5.0.0",
    "nanoid": "^3.3.8",
    "postcss": "^8.4.49",
    "cross-spawn": "^7.0.5",
    "@eslint/plugin-kit": "^0.3.4",
    "esbuild": "^0.25.0",
    "@babel/runtime": "^7.26.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@babel/helpers": "^7.26.10"
  },
  "packageManager": "yarn@4.9.4"
}
